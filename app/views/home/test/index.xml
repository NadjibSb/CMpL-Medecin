            <ScrollView class="vLayout">
                <Label class="calculTaxt RegularFont" text="L('test_calcule_text')"></Label>
                <Require src="/component/buttonImgCentre" id="button_sokal" class="btn" onClick="clickButton"></Require>
                <Require src="/component/buttonImgCentre" id="button_eutos" class="btn" onClick="clickButton"></Require>
                <View class="separator"></View>
                <Label class="calculTaxt RegularFont" text="L('test_tracker_text')"></Label>
