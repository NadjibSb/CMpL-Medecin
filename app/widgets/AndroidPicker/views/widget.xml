<Alloy>
    <View id="vue" onClick="exit">
        <View id="container">
            <View class="insideContainer vLayout">
                <Label id="title" text="title"></Label>
                <View class="separator"></View>
                <ListView id="listView" onItemclick="onItemclick">
                    <Templates>
                        <ItemTemplate name="listTemplate">
                            <View class="hSize itemContainer" module="module/touchView">
                                <Label class="lable" bindId="text"></Label>
                                <ImageView class="icon" bindId="icon"></ImageView>
                            </View>
                        </ItemTemplate>
                    </Templates>
                    <ListSection id="section"></ListSection>
                </ListView>
            </View>
        </View>
    </View>
</Alloy>
